			(function () {
			  function html(s) {
			    return s.replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;');
			  }
			  var newHTML = $('#quine').html();
			 
			  var quineHtml = html($('#quine').html());
			 
			  // Highlight the operative parts:
			  quineHtml = quineHtml.replace(
