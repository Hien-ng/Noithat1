# @BAONGUYEN
# http://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 2
tab_width = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
indent_style = tab
indent_size = 2
tab_width = 2

[{package.json,.travis.yml}]
indent_style = tab
indent_size = 2
tab_width = 2

[**.md]
insert_final_newline = false
trim_trailing_whitespace = false